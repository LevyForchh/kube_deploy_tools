<% return unless config.fetch('test_bool', false) %>
apiVersion: apps/v1beta1
kind: Deployment
metadata:
  namespace: default
  name: test-nginx
  labels:
    tag: "<%= config["tag"] %>"
spec:
  replicas: 0
  template:
    spec:
      containers:
      - name: nginx
        image: nginx
        imagePullPolicy: <%= config["pull_policy"] %>
        ports:
        - name: web
          containerPort: 80

